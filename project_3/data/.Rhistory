wilson <- function(n){
N = 10000
Y_b = 1:N
i = 1
while(i <= N){
X = rnorm(n)
Y = X^2
Y_b[i] = sqrt(n) * (mean(Y)^(1/3) - 1 + 2/(9*n)) / sqrt(2/9)
i = i + 1
}
return(Y_b)
}
